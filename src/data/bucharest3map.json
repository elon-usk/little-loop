{
  "version": 8,
  "name": "Kid Cartoon Map",
  "metadata": {
    "maptiler:copyright": "This style adapted for kid-cartoon look.",
    "maputnik:renderer": "mlgljs"
  },
  "center": [0, 0],
  "zoom": 1,
  "bearing": 0,
  "pitch": 0,
  "sources": {
    "openmaptiles": {
      "url": "https://api.maptiler.com/tiles/v3/tiles.json?key=xwK5OblkSp4XIE9pfl4l",
      "type": "vector"
    },
    "maptiler_attribution": {
      "attribution": "<a href=\"https://www.maptiler.com/copyright/\" target=\"_blank\">&copy; MapTiler</a> <a href=\"https://www.openstreetmap.org/copyright\" target=\"_blank\">&copy; OpenStreetMap contributors</a>",
      "type": "vector"
    }
  },
  "glyphs": "https://api.maptiler.com/fonts/{fontstack}/{range}.pbf?key=xwK5OblkSp4XIE9pfl4l",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": { "background-color": "#FFFAE6" }
    },
    {
      "id": "landuse_residential",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landuse",
      "filter": ["all", ["in", "class", "residential", "suburb", "neighbourhood"]],
      "layout": { "visibility": "visible" },
      "paint": {
        "fill-color": {
          "stops": [
            [6, "#F7E8C9"],
            [14, "#FCEEDB"],
            [18, "#FFF9F0"]
          ]
        },
        "fill-opacity": 0.8
      }
    },
    {
      "id": "landcover_grass",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["in", "class", "grass", "park", "meadow"],
      "layout": { "visibility": "visible" },
      "paint": {
        "fill-color": {
          "stops": [
            [6, "#A8D792"],
            [12, "#8CC57A"],
            [16, "#7BB869"]
          ]
        },
        "fill-opacity": 0.9,
        "fill-outline-color": "#6BA45B"
      }
    },
    {
      "id": "landcover_wood",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["==", "class", "wood"],
      "paint": { "fill-color": "#C3E9B3", "fill-opacity": 0.85 }
    },
    {
      "id": "landcover_sand",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["all", ["in", "class", "sand"]],
      "paint": {
        "fill-color": "#FFE66D",
        "fill-opacity": 0.5,
        "fill-antialias": false
      }
    },
    {
      "id": "landcover_glacier",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "landcover",
      "filter": ["all", ["in", "subclass", "glacier", "ice_shelf"]],
      "layout": { "visibility": "visible" },
      "paint": { "fill-color": "#E6F7FF", "fill-opacity": 1 }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "layout": { "visibility": "visible" },
      "paint": {
        "fill-color": {
          "stops": [
            [6, "#8ED9FF"],
            [12, "#6AD0FF"],
            [16, "#4AC6FF"]
          ]
        },
        "fill-opacity": 0.9,
        "fill-outline-color": "#3AB0CC"
      }
    },
    {
      "id": "water_intermittent",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "filter": ["all", ["==", "intermittent", 1]],
      "layout": { "visibility": "visible" },
      "paint": { "fill-color": "#9FE6FF", "fill-opacity": 0.6 }
    },
    {
      "id": "waterway_tunnel",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "filter": ["all", ["==", "brunnel", "tunnel"]],
      "layout": { "visibility": "visible" },
      "paint": {
        "line-color": "#9FE6FF",
        "line-width": 1,
        "line-opacity": 0.7,
        "line-dasharray": [4, 2]
      }
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "filter": [
        "all",
        ["!in", "brunnel", "tunnel", "bridge"],
        ["!=", "intermittent", 1]
      ],
      "layout": { "visibility": "visible" },
      "paint": {
        "line-color": "#3AB0CC",
        "line-width": {
          "stops": [
            [8, 1],
            [15, 3]
          ]
        },
        "line-opacity": 1
      }
    },
    {
      "id": "waterway_intermittent",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "filter": [
        "all",
        ["!in", "brunnel", "tunnel", "bridge"],
        ["==", "intermittent", 1]
      ],
      "layout": { "visibility": "visible" },
      "paint": {
        "line-color": "#3AB0CC",
        "line-width": {
          "stops": [
            [8, 1],
            [15, 3]
          ]
        },
        "line-opacity": 1,
        "line-dasharray": [3, 1]
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "building",
      "layout": { "visibility": "visible" },
      "paint": {
        "fill-color": "#FFEBB2",
        "fill-outline-color": "#D6C08A",
        "fill-opacity": 0.95
      }
    },
    {
      "id": "road_area_pier",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["==", "$type", "Polygon"], ["==", "class", "pier"]],
      "layout": { "visibility": "visible" },
      "paint": {
        "fill-color": "#FFF1C9",
        "fill-opacity": 1,
        "fill-antialias": true
      }
    },
    {
      "id": "road_area_bridge",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["==", "$type", "Polygon"], ["in", "brunnel", "bridge"]],
      "paint": { "fill-color": "#FFF1C9", "fill-opacity": 0.7 }
    },
    {
      "id": "road_pier",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["in", "class", "pier"]],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": { "line-color": "#FFF1C9", "line-width": 1 }
    },
    {
      "id": "road_path",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 14,
      "filter": ["all", ["in", "class", "path", "track"]],
      "layout": { "line-cap": "square", "line-join": "bevel" },
      "paint": {
        "line-color": "#FFFFFF",
        "line-width": {
          "base": 1.55,
          "stops": [
            [14, 0.5],
            [20, 4]
          ]
        },
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "road_minor",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "residential", "service", "minor"],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#E8D1A2",
        "line-width": {
          "base": 1.4,
          "stops": [
            [12, 0.4],
            [16, 2],
            [20, 10]
          ]
        },
        "line-opacity": 1,
        "line-dasharray": [3, 1]
      }
    },
    {
      "id": "road_bridge",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "brunnel", "bridge"],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#D6B57D",
        "line-width": {
          "base": 1.4,
          "stops": [
            [10, 1],
            [14, 5],
            [20, 20]
          ]
        },
        "line-opacity": 0.9
      }
    },
    {
      "id": "road_major",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["in", "class", "primary", "secondary", "tertiary", "trunk"],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#F2C48F",
        "line-width": {
          "base": 1.4,
          "stops": [
            [8, 1],
            [14, 4],
            [20, 18]
          ]
        },
        "line-opacity": 0.9
      }
    },
    {
      "id": "road_motorway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 4,
      "filter": ["all", ["==", "class", "motorway"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#FFFFFF",
        "line-width": {
          "base": 1.4,
          "stops": [
            [8, 1],
            [16, 10]
          ]
        },
        "line-offset": 0
      }
    },
    {
      "id": "railway_transit_tunnel",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "minzoom": 0,
      "filter": ["all", ["==", "brunnel", "tunnel"], ["==", "class", "transit"]],
      "layout": {
        "line-cap": "butt",
        "line-join": "miter",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#D9D2C5",
        "line-opacity": 0.5,
        "line-dasharray": [4, 2]
      }
    },
    {
      "id": "railway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["==", "class", "rail"],
      "paint": {
        "line-color": "#C7C2BA",
        "line-dasharray": [3, 2],
        "line-width": {
          "base": 1.3,
          "stops": [
            [11, 0.5],
            [18, 2]
          ]
        },
        "line-opacity": 0.7
      }
    },
    {
      "id": "railway_transit",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": ["all", ["==", "class", "transit"], ["!=", "brunnel", "tunnel"]],
      "layout": { "visibility": "visible" },
      "paint": { "line-color": "#D9D2C5", "line-opacity": 0.5 }
    },
    {
      "id": "aeroway_taxiway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "aeroway",
      "minzoom": 12,
      "filter": ["all", ["in", "class", "taxiway"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": { "line-color": "#FFFFFF", "line-width": 1, "line-opacity": 1 }
    },
    {
      "id": "aeroway_runway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "aeroway",
      "minzoom": 4,
      "filter": ["all", ["in", "class", "runway"]],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": { "line-color": "#FFFFFF", "line-width": 5, "line-opacity": 1 }
    },
    {
      "id": "bridge",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["==", "brunnel", "bridge"],
        ["in", "class", "primary", "secondary", "tertiary", "trunk"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#FFFFFF",
        "line-width": {
          "base": 1.4,
          "stops": [
            [7, 0.5],
            [20, 28]
          ]
        }
      }
    },
    {
      "id": "admin_sub",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "minzoom": 3,
      "filter": ["in", "admin_level", 4, 6, 8],
      "layout": { "visibility": "visible" },
      "paint": {
        "line-color": "#B0B0B0",
        "line-width": 1.25,
        "line-dasharray": [3, 2]
      }
    },
    {
      "id": "admin_disputed",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "minzoom": 0,
      "filter": [
        "all",
        ["<=", "admin_level", 2],
        ["==", "$type", "LineString"],
        ["==", "disputed", 1]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#A2A2A2",
        "line-width": 1,
        "line-dasharray": [4, 2]
      }
    },
    {
      "id": "admin_country",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "minzoom": 0,
      "filter": [
        "all",
        ["<=", "admin_level", 2],
        ["==", "$type", "LineString"],
        ["!has", "claimed_by"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round",
        "visibility": "visible"
      },
      "paint": { "line-color": "#A2A2A2", "line-width": 1 }
    },
    {
      "id": "label_airport",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "aerodrome_label",
      "minzoom": 10,
      "filter": ["all", ["has", "iata"]],
      "layout": {
        "icon-size": 1,
        "text-font": ["Comic Sans MS Regular"],
        "text-size": 11,
        "text-field": "{name}",
        "visibility": "visible",
        "text-anchor": "top",
        "text-offset": [0, 0.5],
        "text-max-width": 8
      },
      "paint": {
        "text-color": "#555555",
        "text-halo-blur": 1,
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 1
      }
    },
    {
      "id": "label_road",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "transportation_name",
      "minzoom": 13,
      "filter": ["all", ["==", "$type", "LineString"], ["!=", "subclass", "ferry"]],
      "layout": {
        "text-font": ["Comic Sans MS Regular"],
        "text-size": {
          "base": 1.4,
          "stops": [
            [10, 8],
            [20, 14]
          ]
        },
        "text-field": "{name}",
        "visibility": "visible",
        "text-transform": "uppercase",
        "symbol-placement": "line",
        "text-letter-spacing": 0.1,
        "text-rotation-alignment": "map"
      },
      "paint": {
        "text-color": "#222222",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 2
      }
    },
    {
      "id": "label_place_other",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "minzoom": 8,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["!in", "class", "city", "state", "country", "continent"]
      ],
      "layout": {
        "text-font": ["Comic Sans MS Regular"],
        "text-size": {
          "stops": [
            [6, 10],
            [12, 14]
          ]
        },
        "text-field": "{name}",
        "visibility": "visible",
        "text-anchor": "center",
        "text-max-width": 6
      },
      "paint": {
        "text-color": "#333333",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 1
      }
    },
    {
      "id": "label_place_city",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "maxzoom": 16,
      "filter": ["all", ["==", "$type", "Point"], ["==", "class", "city"]],
      "layout": {
        "text-font": ["Comic Sans MS Bold"],
        "text-size": {
          "stops": [
            [3, 11],
            [8, 16]
          ]
        },
        "text-field": "{name}",
        "visibility": "visible",
        "text-max-width": 10
      },
      "paint": {
        "text-color": "#000000",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 2
      }
    },
    {
      "id": "label_country_other",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "maxzoom": 12,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["==", "class", "country"],
        ["!has", "iso_a2"]
      ],
      "layout": {
        "text-font": ["Comic Sans MS Bold"],
        "text-size": {
          "stops": [
            [3, 12],
            [8, 22]
          ]
        },
        "text-field": "{name:latin}",
        "visibility": "visible",
        "text-max-width": 10
      },
      "paint": {
        "text-color": "#111111",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 2
      }
    },
    {
      "id": "label_country",
      "type": "symbol",
      "source": "openmaptiles",
      "source-layer": "place",
      "maxzoom": 12,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["==", "class", "country"],
        ["has", "iso_a2"]
      ],
      "layout": {
        "text-font": ["Comic Sans MS Bold"],
        "text-size": {
          "stops": [
            [3, 12],
            [8, 22]
          ]
        },
        "text-field": "{name:latin}",
        "visibility": "visible",
        "text-max-width": 10
      },
      "paint": {
        "text-color": "#111111",
        "text-halo-color": "#FFFFFF",
        "text-halo-width": 2
      }
    }
  ],
  "id": "basic-kidcartoon"
}
